{
  "name": "@abi-software/mapintegratedvuer",
  "version": "0.1.28-beta-7",
  "scripts": {
    "serve": "vue-cli-service serve --port 8081",
    "build": "vue-cli-service build --mode prod",
    "test-unit": "mochapack --webpack-config test/unit/webpack.config.js --require test/unit/setup.js test/unit/**/*.spec.js",
    "lint": "vue-cli-service lint",
    "build-bundle": "vue-cli-service build --target lib --name mapintegratedvuer ./src/components/index.js",
    "build-static": "vue-cli-service build --dest test_html --mode static",
    "start": "vue-cli-service serve"
  },
  "main": "./dist/mapintegratedvuer.common.js",
  "files": [
    "assets/*",
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "dependencies": {
    "@abi-software/flatmapvuer": "^0.1.30-beta.3",
    "@abi-software/maptooltip": "^0.1.13",
    "@abi-software/plotvuer": "^0.2.25",
    "@abi-software/scaffoldvuer": "^0.1.44-beta.3",
    "@soda/get-current-script": "^1.0.2",
    "core-js": "^3.3.2",
    "d3-time-format": "^3.0.0",
    "element-ui": "^2.13.0",
    "lodash": "^4.17.20",
    "postcss-prefix-selector": "^1.7.2",
    "shepherd.js": "^7.1.5",
    "splitpanes": "^2.2.1",
    "vue": "^2.6.10",
    "vue-cli-plugin-webpack-bundle-analyzer": "^2.0.0",
    "vue-draggable-resizable": "2.1.0",
    "vue-router": "^3.4.9",
    "vuex": "^3.5.1",
    "webpack-node-externals": "^2.5.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-plugin-unit-mocha": "^4.4.4",
    "@vue/cli-service": "^4.0.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.0.3",
    "babel-plugin-component": "^1.1.1",
    "base64-inline-loader": "^1.1.1",
    "canvas": "^2.6.1",
    "chai": "^4.1.2",
    "dom-parser": "^0.1.6",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "file-loader": "^5.1.0",
    "jsdom": "^16.2.2",
    "jsdom-global": "^3.0.2",
    "mocha": "^8.1.3",
    "mochapack": "^2.0.0",
    "node-loader": "^0.6.0",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^3.1.2",
    "vue-svg-inline-loader": "^1.5.1",
    "vue-template-compiler": "^2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
